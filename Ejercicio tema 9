public class Main {
    public static void main(String[] args) {
        Cliente cliente = new Cliente();
        Trabajador trabajador = new Trabajador();
        cliente.edad = 23;
        cliente.nombre = "Michael";
        cliente.telefono = 32280;
        cliente.credito = 15445;
        trabajador.salario= 1000;
        System.out.println("Soy " + cliente.nombre + " tengo " + cliente.edad+ "años " +"mi credito " +cliente.credito);
        System.out.println("Soy " + cliente.nombre + " tengo " + cliente.edad+ "años " +"mi credito " +cliente.credito+ " mi salarios es " +trabajador.salario);
    }
}
class Persona{
    int edad;
    String nombre;
    int telefono;
}
class Cliente extends Persona{
    int credito;
}
class Trabajador extends Persona{
    int salario;
}
